{
    "name": "clean-structure/clean-structure-symfony-example",
    "description": "Clean Structure: Example base on Symfony",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "ext-zend-opcache": "*",
        "doctrine/dbal": "^4.3.2",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "nelmio/api-doc-bundle": "^5.5",
        "ramsey/uuid": "^4.9",
        "ramsey/uuid-doctrine": "^2.1",
        "symfony/asset": "^7.3.0",
        "symfony/config": "^7.3.2",
        "symfony/console": "^7.3.2",
        "symfony/dependency-injection": "^7.3.2",
        "symfony/dotenv": "^7.3.2",
        "symfony/event-dispatcher": "^7.3",
        "symfony/filesystem": "^7.3.2",
        "symfony/flex": "^2.8.2",
        "symfony/framework-bundle": "^7.3.2",
        "symfony/http-foundation": "^7.3.2",
        "symfony/http-kernel": "^7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/property-access": "^7.3.2",
        "symfony/property-info": "^7.3.1",
        "symfony/routing": "^7.3.2",
        "symfony/runtime": "^7.3.1",
        "symfony/serializer": "^7.3.2",
        "symfony/twig-bundle": "^7.3.2",
        "symfony/yaml": "^7.3.2",
        "zircote/swagger-php": "^5.3.2"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3.1",
        "ergebnis/composer-normalize": "^2.47",
        "phpat/phpat": "^0.11.9",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan-phpunit": "^2.0.7",
        "phpstan/phpstan-strict-rules": "^2.0.6",
        "phpunit/phpunit": "^12.3.6",
        "rector/rector": "^2.1.4",
        "roave/security-advisories": "dev-latest",
        "shipmonk/composer-dependency-analyser": "^1.8.3",
        "squizlabs/php_codesniffer": "^3.13.2",
        "symfony/browser-kit": "^7.3.2",
        "symfony/css-selector": "^7.3",
        "symfony/web-profiler-bundle": "^7.3.2"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CleanStructure\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleanStructure\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "cache-dir": "./var/cache/composer",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "db-migrate": "php bin/console doctrine:migrations:migrate --no-interaction",
        "fix": [
            "composer normalize --diff",
            "composer bump",
            "php vendor/bin/phpcbf",
            "php vendor/bin/rector process"
        ],
        "phpcs": "php vendor/bin/phpcs",
        "phpstan": "php vendor/bin/phpstan --memory-limit=-1",
        "phpunit": "php -dxdebug.mode=coverage vendor/bin/phpunit --coverage-text",
        "rector": "php vendor/bin/rector process --dry-run",
        "test": [
            "composer normalize --diff --dry-run",
            "composer bump --dry-run",
            "composer validate",
            "composer phpcs",
            "composer rector",
            "composer phpstan",
            "php vendor/bin/composer-dependency-analyser",
            "composer outdated --strict --sort-by-age",
            "php bin/console cache:clear",
            "php bin/console lint:container",
            "php bin/console lint:twig --format=github",
            "php bin/console lint:yaml . --format=github",
            "php bin/console nelmio:apidoc:dump --area=default > /dev/null",
            "php bin/console nelmio:apidoc:dump --area=internal > /dev/null",
            "php bin/console doctrine:migrations:migrate --env=test --no-interaction",
            "php bin/console doctrine:schema:validate",
            "composer phpunit"
        ]
    }
}
