parameters:
    level: 8
    paths:
        - bin/
        - config/
        - public/
        - src/
        - tests/
    tmpDir: var/cache/phpstan
    excludePaths:
        - 'tests/CodeSniffer/Sniffs/Commenting/FunctionCommentSniff.php'
services:
   -
       class: Lk2\Tests\Architecture\CleanStructureTest
       tags:
           - phpat.test
   -
       class: Lk2\Tests\Architecture\CleanArchitectureTest
       tags:
           - phpat.test
